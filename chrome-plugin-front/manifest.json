{
  "manifest_version": 3,
  "name": "Smart CV Sender",
  "version": "1.0",
  "description": "A simple extension to send your CV.",
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "permissions": [
    "activeTab",
    "scripting"
  ]
}
